<% include partials/head %>
<% include partials/navbar %>
<% include partials/bgtop %>

<div class="container">
<div class="list-group">
  <a href="#" class="list-group-item active">
    <h3>previous matches</h3>
  </a>
  <% history.forEach( function(historyItem){ %>
      <div class="list-group-item">
        <div class="container">
          <div class="col-md-10">
            <%= historyItem.teamA%> Vs <%= historyItem.teamB%>
          </div>
          <div class="col-md-2">
            <form action = "/scores/<%= historyItem._id %>" method = "post">
              <button type="submit" class="btn btn-info btn-sm">New Match</button>
            </form>
          </div>
        </div>
      </div>
  <% }); %>
</div>
</div>