<% include partials/head %>
<% include partials/navbar %>

<main>
    <div class="panel">
        <div class="panel-heading col-md-8">

            <h3>Latest News</h3>
            <% news_data.articles.forEach(function(news){%>

            <div class="media container col-md-12">
                <div class="media-left media-middle">
                    <a href="<%= news.url%>">
                        <img class="media-object" width="250px" src="<%= news.urlToImage%>" alt="...">
                    </a>
                </div>
                <div class="media-body">
                    <a href="<%= news.url%>"><h4 class="media-heading"><%= news.title%></h4></a>
                    <%= news.description+"...."%>
                </div>
            </div>

            <%});%>
        </div>
        <div class="col-md-4">
            <div class="list-group">
                <a href="#" class="list-group-item list-group-item-info">
                    <img width="30px" src="data:image/svg+xml;utf8;base64," />
                    <strong class=" text-center">Upcoming Matches</strong>
                </a>
               <% match_data.data.slice(0,15).forEach(function(match){%>
                <% var date =  match.date.split(" ")%>
                <% var month = date[1].slice(0,3)%>
                <% date[1] = month;%>
                <% var newDate = date[0]+" "+date[1]+" "+date[2];%>
                <% var i=0;%>
                <% if(i<10){%>
                   <a href="#" class="list-group-item"><%= match['name'].replace(' at',',') %>
                   <span class="pull-right"><%= newDate %></span></a>
               <% i++; %>
                <%}});%>

                <!--<a href="#" class="list-group-item">IND VS AUS <span class="pull-right">Nov 30, 2017</span> </a>-->
                <!--<a href="#" class="list-group-item">IND VS NZ <span class="pull-right">Nov 30, 2017</span> </a>-->
                <!--<a href="#" class="list-group-item">SRI VS PAK <span class="pull-right">Nov 30, 2017</span> </a>-->
                <!--<a href="#" class="list-group-item">WI VS ENG <span class="pull-right">Nov 30, 2017</span> </a>-->
            </div>
        </div>
    </div>
</main>
<% include partials/foot %>